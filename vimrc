inoremap jk <ESC>
inoremap wq <ESC>:wq<ENTER>

inoremap ( ()<ESC>i
inoremap [ []<ESC>i
inoremap { {}<ESC>i
inoremap ' ''<ESC>i
inoremap " ""<ESC>i

nnoremap n nzz
nnoremap N Nzz
nnoremap Y y$
nnoremap U <C-R>

syntax on

set relativenumber
set number
set tabstop=4
set shiftwidth=4
set expandtab
set autoindent
set smartcase
set scrolloff=20
